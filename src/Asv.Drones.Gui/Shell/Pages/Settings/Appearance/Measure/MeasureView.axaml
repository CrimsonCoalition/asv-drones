                <api:OptionsDisplayItem.ActionButton>
                    <ComboBox MinWidth="150" SelectedItem="{Binding SelectedAltitudeUnit}"
                              ItemsSource="{Binding AltitudeUnits}">
                        <ComboBox.ItemTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding Title}" />
                            </DataTemplate>
                        </ComboBox.ItemTemplate>
                    </ComboBox>
